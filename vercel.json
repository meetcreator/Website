{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/Archshield",
            "destination": "/archshield/frontend/out/index.html"
        },
        {
            "source": "/Archshield/",
            "destination": "/archshield/frontend/out/index.html"
        },
        {
            "source": "/Archshield/:path*",
            "destination": "/archshield/frontend/out/:path*"
        },
        {
            "source": "/archshield-app",
            "destination": "/archshield/frontend/out/index.html"
        },
        {
            "source": "/archshield-app/:path*",
            "destination": "/archshield/frontend/out/:path*"
        },
        {
            "source": "/businessanalyticspro",
            "destination": "/businessanalyticspro/Dashboard/frontend/dist/index.html"
        },
        {
            "source": "/businessanalyticspro/",
            "destination": "/businessanalyticspro/Dashboard/frontend/dist/index.html"
        },
        {
            "source": "/businessanalyticspro/:path*",
            "destination": "/businessanalyticspro/Dashboard/frontend/dist/:path*"
        }
    ]
}