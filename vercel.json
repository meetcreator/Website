{
    "cleanUrls": false,
    "redirects": [
        {
            "source": "/Archshield",
            "destination": "/archshield-app/frontend/out/index.html",
            "permanent": false
        },
        {
            "source": "/Archshield/",
            "destination": "/archshield-app/frontend/out/index.html",
            "permanent": false
        },
        {
            "source": "/businessanalyticspro",
            "destination": "/businessanalyticspro/Dashboard/frontend/dist/index.html",
            "permanent": false
        },
        {
            "source": "/businessanalyticspro/",
            "destination": "/businessanalyticspro/Dashboard/frontend/dist/index.html",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/Archshield/:path*",
            "destination": "/archshield-app/frontend/out/:path*"
        },
        {
            "source": "/businessanalyticspro/:path*",
            "destination": "/businessanalyticspro/Dashboard/frontend/dist/:path*"
        }
    ]
}